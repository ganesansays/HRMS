@using HRMS.Models;
@using HRMS.BusinessEntities;

@model ModelContainer<PinCode>

@{
    @Html.Hidden("Id", @Model.Instance.Id)
        
    @Html.LabelFor(model => model.Instance.Code)
    @ControlHelper.RenderTextBoxFormControl(Html, "Code", Model.Instance.Code)
    <br />
    
    @Html.LabelFor(model => model.Instance.Area)
    @ControlHelper.RenderTextBoxFormControl(Html, "Area", Model.Instance.Area)
}
